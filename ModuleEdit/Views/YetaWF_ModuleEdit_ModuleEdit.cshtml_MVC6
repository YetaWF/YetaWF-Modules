@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.ModuleEdit.Modules.ModuleEditModule, YetaWF.Modules.ModuleEdit.Controllers.ModuleEditModuleController.ModuleEditModel>
@using YetaWF.Core.Addons
@using (await CreateFormAsync("ModuleEdit_Partial")) {
    @Html.DisplayFor(m => Model.ModuleGuid)
    @PartialForm()
    @await FormButtonsAsync(new FormButton[] {
        new FormButton() { ButtonType= ButtonTypeEnum.Submit, },
        new FormButton() { ButtonType= ButtonTypeEnum.Cancel, },
    });
}
