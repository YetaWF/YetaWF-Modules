@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.Dashboard.Modules.HttpModulesBrowseModule, YetaWF.Modules.Dashboard.Controllers.HttpModulesBrowseModuleController.BrowseModel>
@if (YetaWF.Core.Support.YetaWFManager.AspNetMvc == YetaWF.Core.Support.YetaWFManager.AspNetMvcVersion.MVC5) {
    @using (Form("HttpModulesBrowse_Partial")) {
        @PartialForm()
        @Html.DisplayFor(m => Model.GridDef)
    }
} else {
    <div class="@YetaWF.Core.Globals.CssDivWarning">
        @__ResStr("notAvail", "Information not available on ASP.NET Core MVC")
    </div>
}