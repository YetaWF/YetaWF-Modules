{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "module": "none",
    "declaration": false,
    "importHelpers": true,
    "skipLibCheck": true,
    "noEmitHelpers": false,
    "jsx": "react",
    "jsxFactory": "$YetaWF.createElement",
    "strict": true,
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "preserveSymlinks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": [
      "YetaWF/HTML/Globals", "YetaWF/HTML/Basics", "YetaWF/HTML/Content", "YetaWF/HTML/Forms", "YetaWF/HTML/Popups", "YetaWF/HTML/Url",
      "YetaWF/HTML/ComponentBase", 
      "YetaWF/HTML/DropDownList/DropDownListEdit"
    ]
  },
  "compileOnSave": true,
  "typeAcquisition": { "enable": false }
}
