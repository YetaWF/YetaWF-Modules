@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.Search.Modules.SearchInputModule, YetaWF.Modules.Search.Controllers.SearchInputModuleController.Model>
@using (await CreateFormAsync("SearchInput_Partial")) {
    @PartialForm()
    @await FormButtonsAsync(new FormButton[] {
        new FormButton() { ButtonType= ButtonTypeEnum.Submit, Text=Module.SearchButtonText, Title=Module.SearchButtonTT },
    });
}
