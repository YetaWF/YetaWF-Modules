@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.SlideShow.Modules.TemplateSlideShowModule, YetaWF.Modules.SlideShow.Controllers.TemplateSlideShowModuleController.Model>
@using (CreateFormAsync("TemplateSlideShow_Partial").Result) {
    @PartialForm()
    @FormButtonsAsync(new FormButton[] {
        new FormButton() { ButtonType= ButtonTypeEnum.Apply, Text=this.__ResStr("btnApply", "Apply") },
        new FormButton() { ButtonType= ButtonTypeEnum.Cancel, },
    }).Result;
}
