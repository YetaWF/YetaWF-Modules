@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.PageEdit.Modules.PageEditModule, YetaWF.Modules.PageEdit.Controllers.PageEditModuleController.EditModel>
@using (Form("PageEdit_Partial")) {
    @Html.DisplayFor(m => Model.PageGuid)
    @PartialForm()
    @FormButtons(new FormButton[] {
        new FormButton() { ButtonType= ButtonTypeEnum.Submit, },
        new FormButton() { ButtonType= ButtonTypeEnum.Cancel, },
    });
}