@inherits YetaWF.Core.Views.RazorView<YetaWF.Modules.BootstrapCarousel.Modules.CarouselDisplayModule, YetaWF.Modules.BootstrapCarousel.Controllers.CarouselDisplayModuleController.Model>
@using YetaWF.Core.Components
@if (Manager.EditMode) {
    using (CreateFormAsync("CarouselDisplay_Partial").Result) {
        @PartialForm()
    }
} else {
    @Html.ForDisplayAsync(Model, "SlideShow").Result
}